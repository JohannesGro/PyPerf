{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
    }
  },
  "handlers": {
    "default": {
      "level": "INFO",
      "formatter": "standard",
      "class": "logging.StreamHandler"
    },
    "file_handler": {
      "level": "INFO",
      "filename": "benchmark.log",
      "class": "logging.FileHandler",
      "formatter": "standard",
      "mode": "w"
    }
  },
  "loggers": {},
  "root": {
    "handlers": ["file_handler", "default"],
    "level": "INFO",
    "propagate": true
  }
}
